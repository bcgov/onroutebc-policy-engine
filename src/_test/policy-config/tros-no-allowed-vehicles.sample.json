        {
          "conditions": {
            "not": {
              "fact": "permitData",
              "path": "$.vehicleDetails.vehicleSubType",
              "operator": "in",
              "value": {
                "fact": "allowedVehicles"
              }
            }
          },
          "event": {
            "type": "violation",
            "params": {
              "message": "Vehicle type not permittable for this permit type",
              "code": "field-validation-error",
              "fieldReference": "permitData.vehicleDetails.vehicleSubType"
            }
          }
        }
      ]
    }
  ],
